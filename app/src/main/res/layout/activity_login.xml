<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.LoginActivity">

    <ImageView
        android:id="@+id/background_image"
        android:layout_width="446dp"
        android:layout_height="495dp"
        android:background="@drawable/vector_1"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-17dp"
        tools:layout_editor_absoluteY="-27dp" />

    <ImageView
        android:id="@+id/image_6"
        android:layout_width="412dp"
        android:layout_height="429dp"
        android:contentDescription="image_6"
        android:src="@drawable/image_6"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="303dp" />

    <ImageView
        android:id="@+id/retro_sneakers_and_shoes_store_logo_2"
        android:layout_width="150dp"
        android:layout_height="129dp"
        android:contentDescription="retro_sneakers_and_shoes_store_logo_2"
        android:src="@drawable/retro_sneakers_and_shoes_store_logo_2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="141dp" />


    <ImageView
        android:id="@+id/vector_ek3"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:contentDescription="vector_ek3"
        android:src="@drawable/vector_ek3"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="16dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/rectangle_1"
        android:layout_width="270dp"
        android:layout_height="78dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:contentDescription="rectangle_1"
        android:src="@drawable/rectangle_1"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="356dp" />

    <ImageView
        android:id="@+id/image_5"
        android:layout_width="21dp"
        android:layout_height="20dp"
        android:contentDescription="image_5"
        android:src="@drawable/image_5"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="406dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/vector_ek7"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:contentDescription="vector_ek7"
        android:src="@drawable/vector_ek7"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="367dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="40dp"
        android:layout_height="20dp"
        android:text="Sigin"
        android:textColorHint="#000000"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="355dp"
        android:textStyle="bold|italic"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:text="Login"
        android:textAlignment="viewStart"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="65dp" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="223dp"
        android:layout_height="33dp"
        android:ems="10"
        android:inputType="text"
        android:text="UserName"
        android:textSize="10dp"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="356dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="224dp"
        android:layout_height="32dp"
        android:ems="10"
        android:inputType="text"
        android:text="PassWord"
        android:textSize="10dp"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="400dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/rectangle_2"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="451dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="70dp"
        android:layout_height="19dp"
        android:gravity="center"
        android:text="Login"
        android:textSize="12dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="451dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="70dp"
        android:layout_height="19dp"
        android:gravity="center"
        android:text="Exit"
        android:textSize="12dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="245dp"
        tools:layout_editor_absoluteY="451dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>